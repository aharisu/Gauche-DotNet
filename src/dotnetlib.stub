(define-cise-stmt raise-error 
  [(_ func-name msg)
   `(Scm_RaiseCondition
      (SCM_OBJ SCM_CLASS_MESSAGE_CONDITION)
      SCM_RAISE_CONDITION_MESSAGE
      "%s: %s"
      ,(symbol->string func-name) ,msg)]
  )

